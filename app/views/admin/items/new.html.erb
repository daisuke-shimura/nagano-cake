<%= link_to "商品一覧", admin_items_path %>
<h3>商品登録</h3>
<%= form_with model: @item, url:admin_items_path, method: :post do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :price %>
  <%= f.number_field :price %>

  <%= f.label :introduction %>
  <%= f.text_area :introduction %>

  <%= f.submit "登録" %>
<% end %>